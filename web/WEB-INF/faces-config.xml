<?xml version='1.0' encoding='UTF-8'?>
<faces-config version="2.2"
              xmlns="http://xmlns.jcp.org/xml/ns/javaee"
              xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
              xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_2.xsd">
<application>
      <locale-config>
         <default-locale>en</default-locale>
      </locale-config>
      <resource-bundle>
         <base-name>resources.messages</base-name>
         <var>msgs</var>
      </resource-bundle>
 </application>
 <navigation-rule>
   	  <from-view-id>/index.xhtml</from-view-id>
      <navigation-case>
         <from-outcome>startOver</from-outcome>
         <to-view-id>/product/index.xhtml</to-view-id>
      </navigation-case>
  </navigation-rule>
  <navigation-rule>
   	  <from-view-id>/*</from-view-id>
      <navigation-case>
         <from-outcome>browseProducts</from-outcome>
         <to-view-id>/product/index.xhtml</to-view-id>
      </navigation-case>
  </navigation-rule>
  <navigation-rule>
   	  <from-view-id>/*</from-view-id>
      <navigation-case>
         <from-outcome>addProduct</from-outcome>
         <to-view-id>/product/add.xhtml</to-view-id>
      </navigation-case>
  </navigation-rule>
  <navigation-rule>
   	  <from-view-id>/*</from-view-id>
      <navigation-case>
         <from-outcome>viewProduct</from-outcome>
         <to-view-id>/product/view.xhtml</to-view-id>
      </navigation-case>
  </navigation-rule>
  <navigation-rule>
   	  <from-view-id>/*</from-view-id>
      <navigation-case>
         <from-outcome>browseOders</from-outcome>
         <to-view-id>/order/index.xhtml</to-view-id>
      </navigation-case>
  </navigation-rule>
  <navigation-rule>
      <from-view-id>/*</from-view-id>
      <navigation-case>
         <from-outcome>register</from-outcome>
         <to-view-id>/customer/register.xhtml</to-view-id>
      </navigation-case>
  </navigation-rule>
  <navigation-rule>
   	  <from-view-id>/*</from-view-id>
      <navigation-case>
         <from-outcome>browseCustomers</from-outcome>
         <to-view-id>/customer/index.xhtml</to-view-id>
      </navigation-case>
  </navigation-rule>
   <navigation-rule>
      <from-view-id>/*</from-view-id>
      <navigation-case>
         <from-outcome>viewCustomer</from-outcome>
         <to-view-id>/customer/view.xhtml</to-view-id>
      </navigation-case>
  </navigation-rule>
   <navigation-rule>
      <from-view-id>/*</from-view-id>
      <navigation-case>
         <from-outcome>viewCart</from-outcome>
         <to-view-id>/cart/view.xhtml</to-view-id>
      </navigation-case>
  </navigation-rule>
</faces-config>
